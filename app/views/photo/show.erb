<img alt="Embedded Image" src="data:image/png;base64,<%=@photo%>" width="600"/>

<%if current_user && @photo_object.album.user_id == current_user.id%>
<form action="/photos/<%=@photo_object.id%>" method="POST">
  <input type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete">
</form>

<form action="/photos/<%=@photo_object.id%>/edit" method="GET">
  <input type="submit" name="GET" value="Edit">
</form>

<%end%>
